<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.Pane?>

<Pane prefHeight="700.0" prefWidth="1200.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.ShoppingCartProductsController">
   <children>
      <TextField fx:id="searchBar" layoutX="500.0" layoutY="30.0" prefWidth="200.0" promptText="Search" />
      <TableView fx:id="tableView" editable="true" layoutX="50.0" layoutY="80.0" prefHeight="500.0" prefWidth="1100.0" tableMenuButtonVisible="true">
         <columns>
            <TableColumn fx:id="imageColumn" editable="false" minWidth="105.0" prefWidth="105.0" resizable="false" sortable="false" text="Image" />
            <TableColumn fx:id="wardColumn" editable="false" minWidth="230.0" prefWidth="230.0" resizable="false" text="Ward" />
            <TableColumn fx:id="nameColumn" editable="false" minWidth="150.0" prefWidth="150.0" resizable="false" text="Name" />
            <TableColumn fx:id="priceColumn" editable="false" minWidth="100.0" prefWidth="100.0" resizable="false" text="Price" />
         </columns>
      </TableView>
   </children>
</Pane>
