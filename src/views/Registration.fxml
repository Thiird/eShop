<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.Pane?>

<Pane fx:id="container" prefHeight="550.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.RegistrationController">
	<children>
		<TextField fx:id="name" layoutX="50.0" layoutY="30.0" prefHeight="25.0" prefWidth="200.0" promptText="Name" />
		<TextField fx:id="surname" layoutX="50.0" layoutY="80.0" prefHeight="25.0" prefWidth="200.0" promptText="Surname" />
		<TextField fx:id="address" layoutX="50.0" layoutY="130.0" prefHeight="25.0" prefWidth="200.0" promptText="Address" />
		<TextField fx:id="CAP" layoutX="50.0" layoutY="180.0" prefHeight="25.0" prefWidth="200.0" promptText="Postal code" />
		<TextField fx:id="city" layoutX="50.0" layoutY="230.0" prefHeight="25.0" prefWidth="200.0" promptText="City" />
		<TextField fx:id="phone" layoutX="50.0" layoutY="280.0" prefHeight="25.0" prefWidth="200.0" promptText="Phone" />
		<TextField fx:id="email" layoutX="50.0" layoutY="330.0" prefHeight="25.0" prefWidth="200.0" promptText="Email" />
		<PasswordField fx:id="password" layoutX="50.0" layoutY="380.0" prefHeight="25.0" prefWidth="200.0" promptText="Password" />
		<CheckBox fx:id="fidelityCard" layoutX="105.0" layoutY="425.0" prefHeight="20.0" prefWidth="90.0" text="Fidelity card" />
		<ComboBox fx:id="paymentMethod" layoutX="55.0" layoutY="464.0" prefHeight="25.0" prefWidth="190.0" promptText="Preferred Payment Method" />
		<Button alignment="CENTER" defaultButton="true" layoutX="115.0" layoutY="510.0" onAction="#checkInput" prefHeight="25.0" prefWidth="70.0" text="Sign up" />
	</children>
</Pane>
